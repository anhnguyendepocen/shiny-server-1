author = {Miranda, M{\^{o}}nica Carolina and Pomp{\'{e}}ia, Sabine and Bueno, Orlando F A},
doi = {10.1590/S1516-44462004000400005},
issn = {1516-4446},
journal = {Revista Brasileira de Psiquiatria},
month = {dec},
number = {4},
pages = {226--233},
title = {{Um estudo comparativo das normas de um conjunto de 400 figuras entre crian{\c{c}}as brasileiras e americanas}},
url = {http://www.scielo.br/scielo.php?script=sci{\_}arttext{\&}pid=S1516-44462004000400005{\&}lng=pt{\&}tlng=pt},
volume = {26},
